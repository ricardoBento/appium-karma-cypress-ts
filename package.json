{
  "name": "flow-hyrbid-app-testing-env",
  "version": "1.0.0",
  "author": "Mapal Os",
  "homepage": "https://mapal-os.com/",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "postinstall": "webdriver-manager update --standalone false --gecko false",
    "cypress:open": "npx cypress open",
    "cypress": "cd node_modules/.bin/cypress && open",
    "cypress-dashboard": "npx cypress run --record --key 84c6a1ba-9821-41c1-aa41-0b1e4e127044",
    "appium": "appium --chromedriver-executable C:/chromedriver_win32/chromedriver.exe",
    "appium-doctor": "appium-doctor",
    "wdio-test": "npx wdio run appium/conf/wdio.conf.js",
    "kill-appium-server": "taskkill /F /IM node.exe"
  },
  "private": true,
  "dependencies": {
    "babel-preset-es2015": "^6.24.1",
    "karma-cli": "^2.0.0",
    "karma-sourcemap-loader": "^0.3.8",
    "karma-spec-reporter": "0.0.32",
    "karma-webpack": "^5.0.0",
    "webpack": "^5.28.0",
    "webpack-cli": "^4.5.0",
    "webpack-karma-jasmine": "^4.0.0"
  },
  "devDependencies": {
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "@wdio/cli": "^5.7.6",
    "@wdio/jasmine-framework": "^5.7.6",
    "@wdio/local-runner": "^5.7.6",
    "@wdio/spec-reporter": "^5.7.2",
    "@wdio/sync": "^5.7.6",
    "appium-uiautomator2-driver": "^1.33.0",
    "babel-preset-env": "^1.7.0",
    "codelyzer": "^6.0.0",
    "cypress": "^6.6.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.2.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "puppeteer": "^8.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2",
    "webdriverio": "^5.7.6"
  },
  "description": "Appium, Karma, Jasmine, Cypress, Protractor and Typecript Testing Env for Cordova Applications"
}
